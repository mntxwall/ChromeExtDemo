{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
],
"icons":{
  "16":"images/idea16.png",
  "32":"images/idea32.png",
  "64":"images/idea64.png",
  "128":"images/idea128.png"
},
"browser_action": {
  "default_icon": "images/ok.png",
  "default_popup": "popup.html"

},
"background": {
  "scripts": ["background.js"]
},
"permissions":[
  "cookies",
  "alarms",
  "tabs",
  "downloads",
  "http://*/*", "https://*/*"
]
}